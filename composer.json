{
    "name": "leeqvip/ldoc",
    "description": "A documentation generator for Laravel.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "JonLee",
            "email": "leeqvip@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "symfony/yaml": "^7.2",
        "erusev/parsedown": "^1.7",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Ldoc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ldoc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ldoc\\LdocServiceProvider"
            ]
        }
    }
}